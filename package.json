{
  "name": "eleven",
  "version": "1.0.0",
  "description": "a electron application",
  "main": "main.js",
  "build": {
    "appId": "com.huan.eleven.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac",
    "distwin": "electron-builder --win --x64"
  },
  "repository": "https://github.com/summer9411/eleven",
  "keywords": [
    "Electron",
    "xlsx",
    "eleven"
  ],
  "author": "zhouhuan",
  "productName": "huan",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4"
  },
  "dependencies": {
    "xlsx": "^0.14.0"
  }
}
